{{!< default}}
<div>
	{{#if queue}}
		<table id="queueTable" class="display hide" style="width:100%">
			<thead>
			<tr>
				<th>Name</th>
				<th>Song</th>
				<th>Artist</th>
				<th>Disk Number</th>
				{{#if isAdmin}}
					<th>Complete</th>
				{{/if}}
			</tr>
			</thead>
			<tbody>
			{{#each queue}}
				<tr>
					<td>{{name}}</td>
					<td>{{title}}</td>
					<td>{{artist}}</td>
					<td>{{disk}}</td>
			{{/each}}
				{{#if isAdmin}}
						<td><input type="checkbox"></td>
				{{/if}}
				</tr>
			</tbody>
		</table>
	{{else}}
		<h2>No active Karaoke sessions.</h2>
	{{/if}}
</div>
